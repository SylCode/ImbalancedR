data <- ubOver(X = train.input, Y = train.output) # ubUnder dziala podobnie
library(C50)
train.output = as.factor(data$Y)
train.input = as.data.frame(data$X)
# model drzewa
treemodel <- C5.0(x = train.input, y = train.output)
summary(treemodel)
